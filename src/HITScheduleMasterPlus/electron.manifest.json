{
  "executable": "HITScheduleMasterPlus",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": false,
  "build": {
    "appId": "com.hcgstudio.HITScheduleMaster.app",
    "productName": "HITScheduleMasterPlus",
    "copyright": "Copyright Â© HCGStudio 2020",
    "buildVersion": "0.1.3",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "../../../icon.ico"
    },
    "mac": {
      "icon": "../../../icon.icns"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": ["**/*"]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": ["**/*"]
      },
      "**/*"
    ]
  }
}